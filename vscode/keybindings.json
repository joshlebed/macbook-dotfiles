// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+d",
    "command": "commandId",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+d",
    "command": "workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled"
  },
  {
    "key": "shift+cmd+enter",
    "command": "editor.action.goToReferences",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+\\",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "shift+cmd+escape",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "alt+n",
    "command": "workbench.action.newGroupRight"
  },
  { "key": "cmd+,", "command": "workbench.action.openSettingsJson" },
  {
    "key": "cmd+shift+,",
    "command": "workbench.action.openGlobalKeybindingsFile"
  },
  { "key": "cmd+j", "command": "workbench.action.terminal.focus" },
  {
    "key": "cmd+j",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  { "key": "cmd+k cmd+enter", "command": "workbench.action.keepEditor" },
  {
    "key": "cmd+alt+b",
    "command": "workbench.action.toggleActivityBarVisibility"
  },
  {
    "key": "cmd+shift+alt+f13",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
  },
  {
    "key": "cmd+shift+alt+f13",
    "command": "toggleSuggestionDetails",
    "when": "suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "cmd+k cmd+m",
    "command": "search",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+w",
    "command": "workbench.action.closeAllGroups"
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.nextEditorInGroup"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.previousEditorInGroup"
  },
  {
    "key": "ctrl+f13",
    "command": "workbench.action.focusPreviousGroup"
  },
  {
    "key": "ctrl+shift+f13",
    "command": "workbench.action.focusNextGroup"
  },
  {
    "key": "cmd+d",
    "command": "editor.action.smartSelect.expand",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+right",
    "command": "-editor.action.smartSelect.expand",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+d",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+left",
    "command": "-editor.action.smartSelect.shrink",
    "when": "editorTextFocus"
  },
  {
    "key": "end",
    "command": "cursorLineEnd"
  },
  {
    "key": "home",
    "command": "cursorLineStart"
  },
  {
    "key": "cmd+enter",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "cmd+shift+enter",
    "command": "editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "shift+f12",
    "command": "-editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "cmd+shift+enter",
    "command": "goToNextReference",
    "when": "inReferenceSearchEditor || referenceSearchVisible"
  },
  {
    "key": "shift+f12",
    "command": "-goToPreviousReference",
    "when": "inReferenceSearchEditor || referenceSearchVisible"
  },
  {
    "key": "cmd+shift+r",
    "command": "workbench.files.action.showActiveFileInExplorer"
  },
  {
    "key": "cmd+shift+r",
    "command": "gitlens.openWorkingFile",
    "when": "isInDiffEditor"
  },
  {
    "key": "cmd+alt+r",
    "command": "workbench.action.reloadWindow",
    "when": "isDevelopment"
  },
  {
    "key": "cmd+shift+j",
    "command": "workbench.action.togglePanel"
  },
  {
    "key": "shift+cmd+g",
    "command": "workbench.view.scm",
    "when": "!activeViewlet"
  },
  {
    "key": "shift+cmd+g",
    "command": "workbench.action.closeSidebar",
    "when": "activeViewlet"
  },
  {
    "key": "PageDown",
    "command": "cursorMove",
    "args": { "to": "viewPortBottom" },
    "when": "editorTextFocus"
  },
  {
    "key": "PageUp",
    "command": "cursorMove",
    "args": { "to": "viewPortTop" },
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+=",
    "command": "editor.action.fontZoomIn"
  },
  {
    "key": "cmd+shift+-",
    "command": "editor.action.fontZoomOut"
  },
  {
    "key": "cmd+[",
    "command": "workbench.action.navigateBack"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.navigateBack"
  },
  {
    "key": "cmd+]",
    "command": "workbench.action.navigateForward"
  },
  {
    "key": "ctrl+shift+-",
    "command": "-workbench.action.navigateForward"
  },
  {
    "key": "shift+cmd+w",
    "command": "workbench.action.closeWindow"
  },
  {
    "key": "shift+cmd+w",
    "command": "-workbench.action.closeWindow"
  },
  {
    "key": "cmd+r",
    "command": "-workbench.action.reloadWindow",
    "when": "isDevelopment"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.openRecent"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.terminal.sendSequence",
    "args": { "text": "\u0012" },
    "when": "terminalFocus"
  },
  {
    "key": "cmd+o",
    "command": "workbench.action.openRecent"
  },
  {
    "key": "ctrl+r",
    "command": "-workbench.action.openRecent"
  },
  {
    "key": "cmd+.",
    "command": "editor.action.showHover",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+.",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+.",
    "command": "-editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
  },
  // folding
  {
    "key": "cmd+k cmd+j",
    "command": "editor.fold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+k cmd+l",
    "command": "editor.unfold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+k cmd+m",
    "command": "editor.foldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+k cmd+.",
    "command": "editor.unfoldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+k alt+cmd+f",
    "command": "editor.foldAll",
    "when": "editorTextFocus && foldingEnabled"
  },

  // other
  {
    "key": "ctrl+alt+cmd+]",
    "command": "search.action.focusNextSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "key": "f4",
    "command": "-search.action.focusNextSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "key": "ctrl+alt+cmd+[",
    "command": "search.action.focusPreviousSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "key": "shift+f4",
    "command": "-search.action.focusPreviousSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "key": "ctrl+g",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "cmd+u",
    "command": "aipopup.action.modal.generate"
  },
  {
    "key": "cmd+u",
    "command": "cursorai.action.generateInTerminal",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  { "key": "cmd+shift+u", "command": "aichat.newchataction" },
  {
    "key": "cmd+l",
    "command": "expandLineSelection",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+j",
    "command": "workbench.action.terminal.focus",
    "when": "aiChatFocus"
  },
  {
    "key": "cmd+l",
    "command": "-aichat.newchataction"
  },
  {
    "key": "ctrl+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "f2",
    "command": "-editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+p",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "cmd+e",
    "command": "composerMode.background"
  },
  {
    "key": "shift+enter",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
      "text": " \\\r\n"
    },
    "when": "terminalFocus"
  },
  {
    "key": "alt+j",
    "command": "workbench.action.terminal.openNativeConsole"
  },
  {
    "key": "cmd+i",
    "command": "claude-code.insertAtMentioned"
  }
]
